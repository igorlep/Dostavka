    UpdateSQL.Strings = (
      'UPDATE USERS_TABLE'
      'SET '
      '    U_NAME = :U_NAME,'
      '    U_LOGIN = :U_LOGIN,'
      '    U_PASSWORD = :U_PASSWORD,'
      '    U_ROLE = :U_ROLE'
